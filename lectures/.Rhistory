mad <- function(df, shuffle=TRUE) {
if (shuffle)
df$PartyGp <- sample(df$PartyGp)
df_means <- df %>% group_by(PartyGp) %>%
summarise(m = mean(Population, na.rm=T))
return(d = abs(df_means$m[1] - df_means$m[2]))
}
